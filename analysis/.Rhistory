filter(vac_status == "Vax") %>%
select(status,time_to_event, time_in_hospital) %>%
tbl_summary(by = status,
statistic = list(all_continuous() ~ "{mean} ({sd})",
all_categorical() ~ "{n} / {N} ({p}%)"),
missing="no") %>%
add_difference() %>%
as_flex_table()
timing_vax
timing_nvax = cov %>%
